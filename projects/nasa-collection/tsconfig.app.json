{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@environments/*": ["./projects/nasa-collection/src/environments/*"],
      "@core/*": ["./projects/nasa-collection/src/app/core/*"],
      "@shared/*": ["./projects/nasa-collection/src/app/shared/*"],
      "@pages/*": ["./projects/nasa-collection/src/app/pages/*"],
      "@modules/*": ["./projects/nasa-collection/src/app/modules/*"]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": true
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.ts"],
  "exclude": ["src/test.ts", "src/**/*.spec.ts"]
}
